import random
import string
pas=int(input("""
1-Capitales letters + numbers.
2-Smole letters + numbers.                
"""))
key=int(input("how many the pass  "))
if pas ==1 or pas==2:
    if pas ==1:
        pas1=random.choices(string.ascii_uppercase+string.digits,k=key)
        print(' '.join(pas1))
    elif pas==2:
        pas2= random.choices(string.ascii_lowercase+string.digits,k=key)
        print(' '.join(pas2))   
else:
    print("please Enter 1 or 2")   
